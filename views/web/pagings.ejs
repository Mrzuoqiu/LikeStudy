<div class="div_page">
  <!--判断不是首页的情况-->
  <% if (!isFirstPage) { %>
  <span class="firstpage"><a title="首页" href="" >首页</a></span>
  <span class="prepage"><a title="上一页" href="?p=<%= page -1 %>">上一页</a></span>
  <% } %>


  <% if( Math.ceil( total /12) < 6) { %>
  <% for(var i = 1,j = Math.ceil( total /12); i <= j; i ++ ) { %>
  <span class="nextpage"><a title="下一页" href="?p=<%= i %>"><%= i %></a></span>
  <% } %>
  <% }else { %>
  <% if(page < 4 ) { %>
  <% for(var i = 1; i <= 5; i ++ ) { %>
  <span class="nextpage"><a title="下一页" href="?p=<%= i %>"><%= i %></a></span>
  <% } %>
  <% }else { %>
  <% for(var i = -2; i <= 2 ; i ++ ) { %>
  <% if(page + i <= Math.ceil(total/12)) { %>
  <span class="nextpage"><a title="下一页" href="?p=<%= page + i %>"><%= page + i %></a></span>
  <% } %>
  <% } %>
  <% } %>
  <% } %>

  <% if (!isLastPage) { %>
  <span class="nextpage"><a title="下一页" href="?p=<%= page + 1 %>">下一页</a></span>
  <span class="lastpage"><a title="尾页" href="?p=<%= Math.ceil( total /12) %>">尾页</a></span>
  <% } %>
</div>
<script>
    $(function(){
        $('.div_page>span').click(function(){
            console.log('00');
            $(this).find('a').addClass('clicked');
        })
    })

</script>